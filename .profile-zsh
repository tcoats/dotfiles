command_exists () {
  type "$1" &> /dev/null ;
}

source ~/.nvm/nvm.sh
source ~/.profile-metocean
if command_exists docker-machine; then
  eval "$(docker-machine env dev)"
fi

if command_exists keychain; then
  eval `keychain --eval assetaltitude_rsa`
fi

alias fuck='$(thefuck $(fc -ln -1))'
alias msg="growlnotify -n 'Message' -m ''"
alias flush="sudo killall -HUP mDNSResponder && msg 'DNS Cache Flushed'"
alias edit="subl /etc/hosts"
alias nuke='docker stop $(docker ps -a -q) && docker rm $(docker ps -a -q)'
alias genesis="tug up consul && sleep 5 && cd consul-backup && consul-restore docker:8500 && cd ../ && tug up proxy && tug up harmony && sleep 1 && tug up mov"

